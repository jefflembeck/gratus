<div class="display">
</div>

<div class="imagestockpile">
  <% @photo_detail.each do |photo| %>
    <div id="slider" class="owl-carousel">
      <div class="item"><img class="lazyOwl" data-src="<%= photo["url"] %>"/></div>
      <br/>
      <%= photo["title"] %>
      <%= photo["tags"] %>
    </div>
  <% end %>
</div>
